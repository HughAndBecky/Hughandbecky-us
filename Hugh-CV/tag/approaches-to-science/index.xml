    &lt;meta itemprop=&#34;givenName&#34; content=&#34;Herman&#34; /&gt;
    H.&lt;/span&gt;&amp;#32;&amp;amp;&amp;#32;&lt;span itemprop=&#34;author&#34; itemscope itemtype=&#34;https://schema.org/Person&#34;&gt;&lt;span itemprop=&#34;familyName&#34;&gt;Lampen&lt;/span&gt;,&amp;#32;
